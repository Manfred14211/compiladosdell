@model IEnumerable<iveCincoFrame.Models.TblSistemasCorreosDestinatariosCorreos>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="row">
    <div class="col-12">
        <div>
            <div class="card">
                <div class="card-body">
                    <div class="my-3">

                        <div id="transac-directorios" class="col-12">
                            <h2>Index</h2>

                            <p>
                                @Html.ActionLink("Create New", "Create")
                            </p>
                            <table class="table">
                                <tr>
                                    <th>
                                        Correo
                                    <th>
                                        Grupo de Correos
                                    </th>
                                    <th>
                                        Fecha de Creacion
                                    </th>
                                    <th>
                                        Usuario Alta
                                    </th>
                                    <th>
                                        Fecha de Modificacion
                                    </th>

                                    <th>
                                        Usuario Modifica
                                    </th>
                                    <th>
                                        Estado
                                    </th>
                                    <th></th>
                                </tr>

                                @foreach (var item in Model)
                                {
                                    <tr>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.CatDestinatariosCorreos.correo)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.CatSistemasCorreos.descripcion)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.fechaCreacion)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.TblUsuarios.usuario)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.fechaModificacion)
                                        </td>

                                        <td>
                                            @Html.DisplayFor(modelItem => item.TblUsuarios1.usuario)
                                        </td>

                                        <td>
                                            @Html.DisplayFor(modelItem => item.CatEstados.descripcion)
                                        </td>
                                        <td>
                                            @Html.ActionLink("Edit", "Edit", new { id = item.idTblSistemasCorreosDestinatariosCorreos }) |
                                            @Html.ActionLink("Delete", "Delete", new { id = item.idTblSistemasCorreosDestinatariosCorreos })
                                        </td>
                                    </tr>
                                }

                            </table>

                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
